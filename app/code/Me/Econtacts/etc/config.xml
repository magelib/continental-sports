<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <econtacts>
            <basic>
                <enabled>1</enabled>
                <modify>0</modify>
            </basic>
            <email>
                <sender_email_identity>custom2</sender_email_identity>
                <email_bcc_enable>0</email_bcc_enable>
                <email_bcc><![CDATA[hello@example.com]]></email_bcc>
                <email_template>econtacts_email_email_template</email_template>
                <include_comment>1</include_comment>
                <include_information>0</include_information>
            </email>
        </econtacts>
    </default>
</config>
